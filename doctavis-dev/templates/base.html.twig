<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Doctavis{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <link href="{{ asset('css/bootstrap/bootstrap.min.css')}}" rel="stylesheet" type="text/css">
            <link href="{{ asset('css/select2.min.css')}}" rel="stylesheet" />
            <link href="{{ asset('css/star-rating.css')}}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('css/global.css')}}" rel="stylesheet" type="text/css">
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="{{ asset('js/bootstrap/bootstrap.bundle.min.js') }}"></script>
            <script src="https://code.jquery.com/jquery-3.6.0.min.js"
                    integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
                    crossorigin="anonymous"></script>
            <script src="{{ asset('js/multipleSelect/multipleSelect.js') }}"></script>
            <script src="{{ asset('js/multipleSelect/select2.min.js') }}"></script>
            <script src="{{ asset('js/dontknow-button.js') }}"></script>
        {% endblock %}
    </head>
    <body>
    {% include "structure/header.html.twig" %}
        {% block body %}
        {% endblock %}
    {% include 'partiels/cookie_consent.html.twig' %}
    {% include "structure/footer.html.twig" %}

    </body>
</html>
